<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.MenuController">
    <stylesheets>
        <URL value="@menu.css" />
    </stylesheets>
   <children>
      <AnchorPane fx:id="mainpanel" prefHeight="200.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="404.0" prefWidth="185.0" style="-fx-background-color: #213555;">
               <children>
                  <ImageView fx:id="img" fitHeight="54.0" fitWidth="200.0">
                     <image>
                        <Image url="@../../images/readytowork-removebg-preview.png" />
                     </image>
                  </ImageView>
                  <Button fx:id="idbtnequipe" mnemonicParsing="false" onAction="#clickeval" prefHeight="51.0" prefWidth="202.0" style="-fx-background-color: #213555;" text="Equipe" textFill="WHITE">
                     <VBox.margin>
                        <Insets bottom="30.0" top="20.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
                    <Button fx:id="idbtnprojet" mnemonicParsing="false" onAction="#clickeval2" prefHeight="51.0" prefWidth="202.0" style="-fx-background-color: #213555;" text="Projet" textFill="WHITE">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="-29.0" />
                     </VBox.margin>
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="idbtntache" mnemonicParsing="false" onAction="#clickeval3" prefHeight="51.0" prefWidth="202.0" style="-fx-background-color: #213555;" text="Tache" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
